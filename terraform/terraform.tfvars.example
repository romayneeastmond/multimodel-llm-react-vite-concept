# Example terraform.tfvars file
# Copy this to terraform.tfvars and update with your values

# Required: Must be globally unique across Azure
cosmos_account_name = "cosmos-multi-model-orch-dev"

# Optional: Override defaults
resource_group_name = "rg-multi-model-orchestrator-dev"
location            = "eastus"
environment         = "dev"

# Enable free tier (only 1 per subscription)
# First 1000 RU/s and 25 GB storage are free
enable_free_tier = false

# Maximum autoscale throughput (RU/s)
# Recommended: 4000 for production, 1000 for dev/testing
max_throughput = 4000
